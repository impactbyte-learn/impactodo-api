{
  "name": "impactodo-api",
  "version": "0.1.0",
  "description": "Impact Todo API with Express, TypeORM, TypeScript",
  "license": "MIT",
  "readmeFilename": "README.md",
  "author": "M Haidar Hanif <npm@mhaidarhanif.com> (https://mhaidarhanif.com)",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node src/index.js",
    "dev": "tsc-watch --onSuccess 'node src/index.js'",
    "build": "tsc",
    "postinstall": "npm run build",
    "new-start": "npm run new-build && cd dist && cross-env NODE_ENV=PRODUCTION node src/index.js",
    "new-dev": "cross-env NODE_ENV=DEV nodemon --watch '**/*.ts' --exec 'ts-node' src/index.ts",
    "new-test": "mocha --timeout 20000 ts-node test/*.ts",
    "new-build": "npm run new-clean && tsc --outDir dist",
    "new-clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/impactbyte-learn/impactodo-api.git"
  },
  "bugs": {
    "url": "https://github.com/impactbyte-learn/impactodo-api/issues"
  },
  "homepage": "https://github.com/impactbyte-learn/impactodo-api#readme",
  "tags": [
    "impactodo",
    "api",
    "orm",
    "typescript",
    "typescript-orm",
    "typeorm-sample",
    "typeorm-example",
    "typeorm-express-example"
  ],
  "engines": {
    "node": "9.x.x",
    "npm": "5.6.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "cross-env": "^5.1.4",
    "mocha": "^5.1.0",
    "mocha-typescript": "^1.1.12",
    "nodemon": "1.17.3",
    "ts-node": "5.0.1",
    "tsc-watch": "^1.0.19",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/express": "^4.11.1",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/method-override": "^0.0.31",
    "@types/node": "^9.6.5",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "express": "^4.15.5",
    "morgan": "^1.9.0",
    "mysql": "^2.14.1",
    "pg": "^7.4.1",
    "reflect-metadata": "^0.1.10",
    "superagent": "^3.8.2",
    "typeorm": "^0.1.0"
  }
}
